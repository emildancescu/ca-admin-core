{
  "name": "ca-admin-core",
  "version": "1.0.11",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "build:lib": "rm -rf dist && mkdir dist && babel src -d dist --ignore 'src/index.js','src/theme.js','src/App.js','src/testModules' --copy-files --no-copy-ignored",
    "prepare": "npm run build:lib",
    "test": "react-app-rewired test",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx,scss,less}\"",
    "eslint": "eslint --ext .js,jsx src",
    "tslint": "tslint \"src/**/*.{ts,tsx}\"",
    "stylelint": "stylelint \"src/**/*.{scss,less}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,scss,less}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{js,jsx}": [
      "eslint"
    ],
    "src/**/*.{scss,less}": [
      "stylelint"
    ]
  },
  "main": "dist/lib.js",
  "files": [
    "dist/*"
  ],
  "babel": {
    "presets": [
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ],
      [
        "babel-plugin-module-resolver",
        {
          "root": [
            "./src/"
          ]
        }
      ],
      [
        "babel-plugin-import",
        {
          "libraryName": "antd",
          "libraryDirectory": "es",
          "style": true
        }
      ]
    ]
  },
  "dependencies": {
    "connected-react-router": "^6.4.0",
    "downloadjs": "^1.4.7",
    "history": "^4.9.0",
    "nprogress": "^0.2.0",
    "rc-drawer": "^1.7.7",
    "react-custom-scrollbars": "^4.2.1",
    "react-loadable": "^5.5.0",
    "react-qr-reader": "^2.2.1",
    "redux-logger": "^3.0.6"
  },
  "peerDependencies": {
    "antd": "^3.26.19",
    "bootstrap-css-only": "^4.2.1",
    "classnames": "^2.2.6",
    "leaflet": "^1.5.1",
    "leaflet-ant-path": "^1.3.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "prop-types": "^15.7.1",
    "qs": "^6.6.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-helmet": "^5.2.0",
    "react-intl": "^5.4.5",
    "react-leaflet": "^2.4.0",
    "react-leaflet-ant-path": "^1.1.0",
    "react-leaflet-fullscreen": "^1.0.1",
    "react-redux": "^6.0.0",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.1",
    "redux-thunk": "^2.3.0",
    "store": "^2.0.12"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/preset-react": "^7.9.4",
    "antd": "^3.26.20",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "bootstrap-css-only": "^4.2.1",
    "classnames": "^2.2.6",
    "customize-cra": "^0.2.11",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "husky": "^1.3.1",
    "leaflet": "^1.5.1",
    "leaflet-ant-path": "^1.3.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "lint-staged": "^8.1.3",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "node-sass": "^4.11.0",
    "prettier": "^1.17.1",
    "prop-types": "^15.7.1",
    "qs": "^6.6.0",
    "react": "^16.8.1",
    "react-app-rewired": "^2.1.0",
    "react-dom": "^16.8.1",
    "react-helmet": "^5.2.0",
    "react-intl": "^5.4.5",
    "react-leaflet": "^2.4.0",
    "react-leaflet-ant-path": "^1.1.0",
    "react-leaflet-fullscreen": "^1.0.1",
    "react-redux": "^6.0.0",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.5",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.1",
    "redux-thunk": "^2.3.0",
    "store": "^2.0.12",
    "stylelint": "^9.10.1",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.2.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
